#!/bin/bash
APP_NAME="luci-app-smartdns"
svn co https://github.com/Lienol/openwrt-luci/trunk/applications/luci-app-smartdns package/luci-app-smartdns
svn co https://github.com/Lienol/openwrt-packages/trunk/net/smartdns package/luci-app-smartdns/smartdns
pushd package/luci-app-smartdns/smartdns
make && sudo make install
popd
